{
  "attributes": {
    "dataSource.vendor": "Forcepoint",
    "dataSource.name": "Forcepoint Firewall",
    "dataSource.category": "network",
    "metadata.product.vendor_name": "Forcepoint",
    "metadata.product.name": "Forcepoint Firewall",
    "metadata.version": "1.0.0"
  },
  "formats": [
    {
      "format": "$unmapped.{parse=json}$",
      "rewrites": [
        {
          "input": "unmapped.timestamp",
          "output": "timestamp",
          "match": ".*",
          "replace": "$0"
        }
      ]
    }
  ],
  "mappings": {
    "version": 1,
    "mappings": [
      {
        "predicate": "true",
        "transformations": [
          {
            "constant": {
              "value": 4001,
              "field": "class_uid"
            }
          },
          {
            "constant": {
              "value": "Network Activity",
              "field": "class_name"
            }
          },
          {
            "constant": {
              "value": 4,
              "field": "category_uid"
            }
          },
          {
            "constant": {
              "value": "Network Activity",
              "field": "category_name"
            }
          },
          {
            "copy": {
              "from": "unmapped.timestamp",
              "to": "time"
            }
          },
          {
            "cast": {
              "field": "time",
              "type": "iso8601TimestampToEpochSec"
            }
          },
          {
            "copy": {
              "from": "unmapped.connection_id",
              "to": "metadata.uid"
            }
          },
          {
            "copy": {
              "from": "unmapped.action",
              "to": "message"
            }
          },
          {
            "copy": {
              "from": "unmapped.source_ip",
              "to": "src_endpoint.ip"
            }
          },
          {
            "copy": {
              "from": "unmapped.source_port",
              "to": "src_endpoint.port"
            }
          },
          {
            "copy": {
              "from": "unmapped.destination_ip",
              "to": "dst_endpoint.ip"
            }
          },
          {
            "copy": {
              "from": "unmapped.destination_port",
              "to": "dst_endpoint.port"
            }
          },
          {
            "copy": {
              "from": "unmapped.protocol",
              "to": "connection_info.protocol_name"
            }
          },
          {
            "copy": {
              "from": "unmapped.rule_name",
              "to": "metadata.extensions.rule_name"
            }
          },
          {
            "copy": {
              "from": "unmapped.bytes_sent",
              "to": "traffic.bytes_out"
            }
          },
          {
            "copy": {
              "from": "unmapped.bytes_received",
              "to": "traffic.bytes_in"
            }
          },
          {
            "constant": {
              "value": 1,
              "field": "activity_id",
              "predicate": "unmapped.action = 'Allow'"
            }
          },
          {
            "constant": {
              "value": 2,
              "field": "activity_id",
              "predicate": "unmapped.action = 'Block'"
            }
          },
          {
            "constant": {
              "value": 1,
              "field": "severity_id"
            }
          },
          {
            "constant": {
              "value": 1,
              "field": "status_id"
            }
          }
        ]
      }
    ]
  },
  "observables": {
    "fields": [
      {
        "name": "src_endpoint.ip",
        "type": "IP Address"
      },
      {
        "name": "dst_endpoint.ip",
        "type": "IP Address"
      }
    ]
  }
}
