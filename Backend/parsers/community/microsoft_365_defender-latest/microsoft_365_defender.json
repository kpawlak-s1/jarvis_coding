{
    "attributes": {
        "dataSource.name": "Microsoft 365 Defender",
        "dataSource.vendor": "Microsoft",
        "dataSource.category": "security",
        "metadata.product.name": "Microsoft 365 Defender for Endpoint",
        "metadata.product.vendor_name": "Microsoft",
        "metadata.version": "1.0.0"
    },
    "formats": [
        {
            "format": "$unmapped.{parse=gron}$",
            "rewrites": [
                {
                    "input": "unmapped.Timestamp",
                    "output": "timestamp",
                    "match": ".*",
                    "replace": "$0"
                }
            ]
        }
    ],
    "mappings": {
        "version": 1,
        "mappings": [
            {
                "predicate": "true",
                "transformations": [
                    {
                        "constant": {
                            "value": 1,
                            "field": "activity_id"
                        }
                    },
                    {
                        "constant": {
                            "value": "Launch",
                            "field": "activity_name"
                        }
                    },
                    {
                        "constant": {
                            "value": 1,
                            "field": "category_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": 1007,
                            "field": "class_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": "Process Activity",
                            "field": "class_name"
                        }
                    },
                    {
                        "constant": {
                            "value": "System Activity",
                            "field": "category_name"
                        }
                    },
                    {
                        "constant": {
                            "value": 100701,
                            "field": "type_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": "Process Activity: Launch",
                            "field": "type_name"
                        }
                    },
                    {
                        "copy": {
                            "from": "unmapped.Timestamp",
                            "to": "time"
                        }
                    },
                    {
                        "cast": {
                            "field": "time",
                            "type": "iso8601TimestampToEpochSec"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.DeviceName",
                            "to": "device.hostname"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.DeviceId",
                            "to": "device.uid"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.AccountName",
                            "to": "actor.user.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.AccountDomain",
                            "to": "actor.user.domain"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.ActionType",
                            "to": "activity_name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.FileName",
                            "to": "file.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.FolderPath",
                            "to": "file.path"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.SHA1",
                            "to": "file.hashes.sha1"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.MD5",
                            "to": "file.hashes.md5"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.DetectionId",
                            "to": "malware.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.ProcessName",
                            "to": "process.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.ProcessId",
                            "to": "process.pid"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.ProcessCommandLine",
                            "to": "process.command_line"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.ParentProcessId",
                            "to": "process.parent_process.pid"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.ParentProcessName",
                            "to": "process.parent_process.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.RemoteIP",
                            "to": "dst_endpoint.ip"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.RemotePort",
                            "to": "dst_endpoint.port"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.RemoteUrl",
                            "to": "dst_endpoint.url.url_string"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.Protocol",
                            "to": "connection_info.protocol_name"
                        }
                    }
                ]
            }
        ]
    }
}