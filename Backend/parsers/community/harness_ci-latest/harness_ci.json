      "rewrites": [
        {
          "input": "timestamp",
          "output": "timestamp",
          "match": ".*",
          "replace": "$0"
        },
        {
          "input": "pipeline_id",
          "output": "job.name",
          "match": ".*",
          "replace": "$0"
        },
        {
          "input": "execution_id",
          "output": "job.uid",
          "match": ".*",
          "replace": "$0"
        },
        {
          "input": "status",
          "output": "status_detail",
          "match": ".*",
          "replace": "$0"
        },
        {
          "input": "trigger",
          "output": "job.run_type",
          "match": ".*",
          "replace": "$0"
        },
        {
          "input": "initiator",
          "output": "actor.user.name",
          "match": ".*",
          "replace": "$0"
        },
        {
          "input": "message",
          "output": "message",
          "match": ".*",
          "replace": "$0"
        }
      ]
    }
  ],
