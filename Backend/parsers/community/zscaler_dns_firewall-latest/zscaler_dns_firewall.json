{
    "attributes": {
        "dataSource.name": "Zscaler DNS Firewall",
        "dataSource.vendor": "Zscaler",
        "dataSource.category": "security",
        "metadata.product.name": "Zscaler Internet Access DNS Firewall",
        "metadata.product.vendor_name": "Zscaler",
        "metadata.version": "1.0.0"
    },
    "formats": [
        {
            "format": "$unmapped.{parse=gron}$",
            "rewrites": [
                {
                    "input": "unmapped.timestamp",
                    "output": "timestamp",
                    "match": ".*",
                    "replace": "$0"
                }
            ]
        }
    ],
    "mappings": {
        "version": 1,
        "mappings": [
            {
                "predicate": "true",
                "transformations": [
                    {
                        "constant": {
                            "value": 6,
                            "field": "activity_id"
                        }
                    },
                    {
                        "constant": {
                            "value": "Traffic",
                            "field": "activity_name"
                        }
                    },
                    {
                        "constant": {
                            "value": 4,
                            "field": "category_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": 4003,
                            "field": "class_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": "DNS Activity",
                            "field": "class_name"
                        }
                    },
                    {
                        "constant": {
                            "value": "Network Activity",
                            "field": "category_name"
                        }
                    },
                    {
                        "constant": {
                            "value": 400306,
                            "field": "type_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": "DNS Activity: Traffic",
                            "field": "type_name"
                        }
                    },
                    {
                        "copy": {
                            "from": "unmapped.timestamp",
                            "to": "time"
                        }
                    },
                    {
                        "cast": {
                            "field": "time",
                            "type": "iso8601TimestampToEpochSec"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.userName",
                            "to": "actor.user.email_addr"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.sourceIp",
                            "to": "src_endpoint.ip"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.deviceId",
                            "to": "device.uid"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.query",
                            "to": "query.hostname"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.queryType",
                            "to": "query.type"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.responseCode",
                            "to": "rcode"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.answer",
                            "to": "answers"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.action",
                            "to": "disposition"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.policyId",
                            "to": "policy.uid"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.threatCategory",
                            "to": "malware.classification_ids"
                        }
                    },
                    {
                        "constant": {
                            "value": "Zscaler",
                            "field": "cloud.provider"
                        }
                    }
                ]
            }
        ]
    }
}