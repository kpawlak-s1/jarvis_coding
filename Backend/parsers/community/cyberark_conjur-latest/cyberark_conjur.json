{
    "attributes": {
        "dataSource.name": "CyberArk Conjur",
        "dataSource.vendor": "CyberArk",
        "dataSource.category": "security",
        "metadata.product.name": "Conjur Secrets Management",
        "metadata.product.vendor_name": "CyberArk",
        "metadata.version": "1.0.0"
    },
    "formats": [
        {
            "format": "$unmapped.{parse=gron}$",
            "rewrites": [
                {
                    "input": "unmapped.timestamp",
                    "output": "timestamp",
                    "match": ".*",
                    "replace": "$0"
                }
            ]
        }
    ],
    "mappings": {
        "version": 1,
        "mappings": [
            {
                "predicate": "true",
                "transformations": [
                    {
                        "constant": {
                            "value": 1,
                            "field": "activity_id"
                        }
                    },
                    {
                        "constant": {
                            "value": "Logon",
                            "field": "activity_name"
                        }
                    },
                    {
                        "constant": {
                            "value": 3,
                            "field": "category_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": 3002,
                            "field": "class_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": "Authentication",
                            "field": "class_name"
                        }
                    },
                    {
                        "constant": {
                            "value": "Identity & Access Management",
                            "field": "category_name"
                        }
                    },
                    {
                        "constant": {
                            "value": 300201,
                            "field": "type_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": "Authentication: Logon",
                            "field": "type_name"
                        }
                    },
                    {
                        "copy": {
                            "from": "unmapped.timestamp",
                            "to": "time"
                        }
                    },
                    {
                        "cast": {
                            "field": "time",
                            "type": "iso8601TimestampToEpochSec"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.hostname",
                            "to": "src_endpoint.hostname"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.operation",
                            "to": "activity_name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.result",
                            "to": "status"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.role",
                            "to": "actor.user.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.authenticator",
                            "to": "auth_protocol"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.user",
                            "to": "user.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.resource_id",
                            "to": "resource.uid"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.secret_id",
                            "to": "resource.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.privileges",
                            "to": "privileges"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.message",
                            "to": "message"
                        }
                    }
                ]
            }
        ]
    }
}