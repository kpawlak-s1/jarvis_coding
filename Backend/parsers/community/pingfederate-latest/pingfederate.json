{
    "attributes": {
        "dataSource.name": "PingFederate",
        "dataSource.vendor": "PingIdentity",
        "dataSource.category": "security",
        "metadata.product.name": "PingFederate",
        "metadata.product.vendor_name": "PingIdentity",
        "metadata.version": "1.0.0"
    },
    "formats": [
        {
            "format": "$unmapped.{parse=gron}$",
            "rewrites": [
                {
                    "input": "unmapped.timestamp",
                    "output": "timestamp",
                    "match": ".*",
                    "replace": "$0"
                }
            ]
        }
    ],
    "mappings": {
        "version": 1,
        "mappings": [
            {
                "predicate": "true",
                "transformations": [
                    {
                        "constant": {
                            "value": 1,
                            "field": "activity_id"
                        }
                    },
                    {
                        "constant": {
                            "value": "Logon",
                            "field": "activity_name"
                        }
                    },
                    {
                        "constant": {
                            "value": 3,
                            "field": "category_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": 3002,
                            "field": "class_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": "Authentication",
                            "field": "class_name"
                        }
                    },
                    {
                        "constant": {
                            "value": "Identity & Access Management",
                            "field": "category_name"
                        }
                    },
                    {
                        "constant": {
                            "value": 300201,
                            "field": "type_uid"
                        }
                    },
                    {
                        "constant": {
                            "value": "Authentication: Logon",
                            "field": "type_name"
                        }
                    },
                    {
                        "copy": {
                            "from": "unmapped.timestamp",
                            "to": "time"
                        }
                    },
                    {
                        "cast": {
                            "field": "time",
                            "type": "iso8601TimestampToEpochSec"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.log_level",
                            "to": "severity"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.logger_name",
                            "to": "class_name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.username",
                            "to": "user.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.client_ip",
                            "to": "src_endpoint.ip"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.status",
                            "to": "status"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.operation",
                            "to": "activity_name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.message",
                            "to": "message"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.session_id",
                            "to": "session.uid"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.client_id",
                            "to": "actor.application.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.scope",
                            "to": "authorization.scope"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.target_application",
                            "to": "dst_application.name"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.adapter_id",
                            "to": "metadata.extension.adapter_id"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.token_type",
                            "to": "metadata.extension.token_type"
                        }
                    },
                    {
                        "rename": {
                            "from": "unmapped.transaction_id",
                            "to": "metadata.extension.transaction_id"
                        }
                    }
                ]
            }
        ]
    }
}